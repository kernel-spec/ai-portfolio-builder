{
  "version": "1.1.0",
  "governance_mode": "hybrid",

  "hash_enforcement": {
    "ui_layer": "soft-validation",
    "worker_layer": "hard-attestation",
    "ci_layer": "mandatory"
  },

  "taxonomy": {
    "domains": 10,
    "archetypes": 4,
    "immutability": true
  },

  "security": {
    "attestation_required_for_production_dispatch": true,
    "fail_closed": true,
    "unknown_agent_blocked": true,
    "runtime_hash_verification": true
  },

  "compatibility": {
    "custom_gpt_runtime": "supported",
    "ios_runtime": "supported",
    "openapi_dispatch": "required"
  },

  "audit": {
    "lock_file_required": true,
    "ci_hash_validation": true,
    "worker_logging_enabled": true
  }
}