{
  "version": "1.1.0",
  "governance_mode": "hybrid",
  "governance": {
    "model": "lockfile-enforced",
    "immutable_between_versions": true,
    "fail_closed": true,
    "runtime_integrity_required": true
  },
  "taxonomy": {
    "domains": [
      "strategy",
      "finance",
      "product",
      "growth",
      "operations",
      "legal",
      "technology",
      "data",
      "marketing",
      "governance"
    ],
    "archetypes": [
      "founder",
      "operator",
      "investor",
      "board"
    ],
    "total_prompts": 14
  },
  "security": {
    "fail_closed_on_unknown_agent": true,
    "no_runtime_prompt_mutation": true,
    "no_client_hash_override": true,
    "hash_algorithm": "sha256"
  },
  "compatibility": {
    "minimum_worker_version": "1.1.0",
    "lockfile_required": true
  }
}